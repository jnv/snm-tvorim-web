{
  "name": "snm-tvorim-web",
  "version": "1.0.0",
  "private": true,
  "description": "Materiály k workshopu Tvořím web: HTML a CSS",
  "scripts": {
    "start": "reveal-md slides.md --disable-auto-open",
    "build": "npm run build:css && npm run build:slides",
    "build:css": "node-sass theme/czechitas.scss theme/czechitas.css",
    "build:slides": "rimraf _static && reveal-md slides.md --static _static"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jnv/snm-tvorim-web.git"
  },
  "devDependencies": {
    "node-sass": "^4.12.0",
    "reveal-md": "^3.0.4",
    "rimraf": "^2.6.3"
  },
  "license": "MIT"
}
